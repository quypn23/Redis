init:
    image: redis:latest
    command: bash -c "sleep 30 && redis-cli --cluster create redis-master1:6379 redis-master2:6380 redis-master3:6381 redis-slave-1:6379 redis-slave-2:6379 redis-slave-3:6379 --cluster-replicas 1"
    depends_on:
      - redis-master1
      - redis-master2
      - redis-master3
      - redis-slave-1
      - redis-slave-2
      - redis-slave-3
    networks:
      - redis-net